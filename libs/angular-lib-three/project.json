{
  "name": "@gipo999/angular-lib-three",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular-lib-three/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/{projectRoot}/dist"],
      "options": {
        "project": "libs/angular-lib-three/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/angular-lib-three/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/angular-lib-three/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/angular-lib-three/jest.config.ts"
      }
    },
    "fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint --fix ./src && prettier --write -c ./src",
        "cwd": "libs/angular-lib"
      }
    },
    "pre-lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p ./tsconfig.app.json & prettier -c ./src",
        "cwd": "libs/angular-lib"
      }
    },
    "lint": {
      "dependsOn": ["pre-lint"],
      "executor": "@nx/eslint:lint"
    }
  }
}
