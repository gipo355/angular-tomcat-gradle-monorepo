{
  "name": "angular-lib",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/angular-lib/src",
  "prefix": "lib",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "dist/{projectRoot}",
        "currentVersionResolver": "git-tag"
      }
    }
  },
  "tags": [],
  "targets": {
    "fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint --fix ./src && prettier --write -c ./src",
        "cwd": "libs/angular-lib"
      }
    },
    "pre-lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit -p ./tsconfig.app.json & prettier -c ./src",
        "cwd": "libs/angular-lib"
      }
    },
    "lint": {
      "dependsOn": ["pre-lint"],
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/angular-lib/jest.config.ts"
      }
    },
    "nx-release-publish": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "echo 'nx-release-publish'"
      }
    }
  }
}
